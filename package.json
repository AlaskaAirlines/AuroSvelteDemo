{
  "name": "svelte-app",
  "version": "2.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "@alaskaairux/auro-button": "^6.3.4",
    "@alaskaairux/auro-checkbox": "^1.1.2",
    "@alaskaairux/auro-header": "^1.5.1",
    "@alaskaairux/auro-input": "^1.3.5",
    "@alaskaairux/auro-radio": "^1.5.0",
    "@alaskaairux/design-tokens": "^3.3.0",
    "@alaskaairux/webcorestylesheets": "^3.4.0",
    "@babel/core": "^7.14.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "core-js": "^3.15.2",
    "focus-visible": "^5.2.0",
    "husky": "^6.0.0",
    "node-sass": "^9.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^17.4.4",
    "shimport": "^1.0.1",
    "svelte": "^3.40.3",
    "svelte-preprocess": "^3.9.12"
  },
  "dependencies": {
    "sirv-cli": "^0.4.6"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md",
          "changelogTitle": "# Semantic Release Automated Changelog"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "./CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "Jason Baker",
      "email": "jason.baker@alaskaair.com"
    },
    {
      "name": "Dale Sande",
      "email": "dale.sande@alaskaair.com"
    },
    {
      "name": "Geoff Rich",
      "email": "geoff.rich@alaskaair.com"
    }
  ],
  "browserslist": [
    "last 3 chrome version",
    "last 3 firefox version",
    "last 3 edge version",
    "last 2 safari major version"
  ]
}
